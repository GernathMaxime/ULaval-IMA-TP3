<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ca.ulaval.ima.tp3">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TP3">
        <activity android:name=".AllModelListActivity"></activity>
        <activity android:name=".SearchListActivity" />
        <activity android:name=".ModelListActivity" />
        <activity android:name=".ProfilCarActivity" />
        <activity android:name=".customComponant.SpinnerActivity" />
        <activity
            android:name=".customComponant.EditTextActivity"
            android:label="@string/title_activity_edit_text"
            android:theme="@style/Theme.TP3.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.TP3.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>